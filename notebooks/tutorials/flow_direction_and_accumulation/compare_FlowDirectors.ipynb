   "source": [
    "mg1a = RasterModelGrid((10, 10))\n",
    "_ = mg1a.add_field(\"topographic__elevation\", mg1a.y_of_node, at=\"node\")\n",
    "ar = mg1a.at_node['topographic__elevation'].copy()\n",
    "ar[20:30] = 3.5\n",
